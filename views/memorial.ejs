    <header class="masthead" style="background-image:url('assets/img/intro-bg.jpg');">
        <div class="intro-body">
            <div class="container">
                <div class="row">
                    <div class="col-lg-8 mx-auto">
                        <h1 class="brand-heading">Memorial</h1>
                        <p class="intro-text">Remembering those whose lives were lost during the series of terrible
                            attacks</p><a class="btn btn-link btn-circle js-scroll-trigger" role="button"
                            href="#about"><i class="fa fa-angle-double-down animated"></i></a>
                    </div>
                </div>
